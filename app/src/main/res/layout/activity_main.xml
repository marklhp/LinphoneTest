<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="click"
            type="android.view.View.OnClickListener" />
        <variable
            name="initState"
            type="String" />
        <variable
            name="registerState"
            type="String" />
        <variable
            name="callstate"
            type="String" />
    </data>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:context=".MainActivity">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:textSize="16sp"
                android:text="@{initState}"
                android:hint="初始化状态"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:textSize="16sp"
                android:text="@{registerState}"
                android:hint="注册状态"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:textSize="16sp"
                android:text="@{callstate}"
                android:hint="通话状态"
                />
            <TextView
                android:id="@+id/linphone_tv1"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="1初始化"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv2"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="2打电话"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv3"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="3挂断"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv4"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="4接电话"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv5"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="5静音"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv6"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="6免提"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv7"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="7退出登录"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv8"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="8按键声音"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv9"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="9按键值发送"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv10"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="10获取通话显示号码"
                android:gravity="center"
                android:onClick="@{click}"
                />

            <TextView
                android:id="@+id/linphone_tv11"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="11获取通话号码"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv12"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="12获取通话地址"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv13"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="13获取通话时长"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv14"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="14获取登录账户代理"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv15"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="15获取登录账户域名"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv16"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="16再添加监听"
                android:gravity="center"
                android:onClick="@{click}"
                />
            <TextView
                android:id="@+id/linphone_tv17"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="打印日志"
                android:gravity="center"
                android:onClick="@{click}"
                />
        </LinearLayout>
    </ScrollView>
</layout>